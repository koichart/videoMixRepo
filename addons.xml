<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<addons>
<addon id="plugin.audio.binaural"
       name="Bin@ural"
       version="1.0.0"
       provider-name="Kinkin, art by Christara">
    <requires>
        <import addon="xbmc.python" version="2.1.0"/>
        <import addon="script.module.requests" version="1.1.0"/>
        <import addon="script.module.t0mm0.common" version="1.1.0"/>
        <import addon="plugin.video.youtube"/>
    </requires>
    <extension point="xbmc.python.pluginsource"
               	library="default.py">
        <provides>audio video</provides>
    </extension>
    <extension point="xbmc.addon.metadata">
        <platform>all</platform>
        <summary>Audio and Video Music Streaming</summary>
        <description>Browse and stream your favourite songs and albums and videos</description>
    </extension>

</addon>

<addon id="plugin.video.filmdictator" name="Film Dictator" version="2.0.1" provider-name="MetalKettle">
  <requires>
    <import addon="script.module.simplejson" version="3.3.0"/>
    <import addon="xbmc.python" version="2.1.0"/>
    <import addon="script.module.t0mm0.common" version="2.0.0"/>
    <import addon="plugin.video.youtube" version="3.0.0"/>
    <import addon="script.module.urlresolver"  version="2.1.2"/>
    <import addon="script.common.plugin.cache" version="2.5.2"/>
    <import addon="script.module.metahandler" version="2.5.1"/>
  </requires>
  <extension point="xbmc.python.pluginsource" library="default.py">
        <provides>video</provides>
  </extension>
  <extension point="xbmc.addon.metadata">
    <summary lang="en"></summary>
    <description lang="en"></description>
    <platform>all</platform>
  </extension>
</addon>

<addon id="plugin.video.genesis" name="Genesis" version="5.1.1" provider-name="lambda">
	<requires>
		<import addon="xbmc.python" version="2.1.0" />
		<import addon="script.module.requests" version="1.0.0" />
		<import addon="script.module.metahandler" optional="true" />
		<import addon="plugin.video.youtube" optional="true" />
	</requires>
	<extension point="xbmc.python.pluginsource" library="default.py">
		<provides>video</provides>
	</extension>
	<extension point="xbmc.service" library="service.py" start="startup" />
	<extension point="xbmc.addon.metadata">
		<summary lang="en">The origins of streaming</summary>
		<description lang="en">Visit TVADDONS.ag for support</description>
		<disclaimer lang="en">The author does not host or distribute any of the content displayed by this addon. The author does not have any affiliation with the content provider.</disclaimer>
		<forum>http://forums.tvaddons.ag/forums/148-lambda-s-xbmc-addons</forum>
		<source>https://offshoregit.com/lambda81/</source>
		<website>https://offshoregit.com/lambda81/</website>
		<platform>all</platform>
	</extension>
</addon>

<addon id="plugin.video.mvmix" version="1.3.0" name="Music Video Mix" provider-name="Jin">
    <requires>
        <import addon="xbmc.python" version="2.1.0"/>
    </requires>
    <extension point="xbmc.python.pluginsource" library="mvmix.py">
        <provides>video</provides>
    </extension>
    <extension point="xbmc.addon.metadata">
        <summary>Music Video Mix Player</summary>
        <description>Randomly Play Local And Online Music Videos Based On The Last FM Similar Artists API</description>
        <platform>all</platform>
        <forum>http://kodinerds.net</forum>
    </extension>
</addon>

<addon id="repository.videomixrepo" name="videomixrepo" version="1.0.0" provider-name="Koicha">
	<extension point="xbmc.addon.repository" name="ToledoRepoMix">
		<info compressed="false">https://raw.github.com/koicha/videomixrepo/master/addons.xml</info>
		<checksum>https://raw.github.com/koicha/videomixrepo/master/addons.xml.md5</checksum>
		<datadir zip="true">https://raw.github.com/koicha/videomixrepo/master</datadir>
	</extension>
	<extension point="xbmc.addon.metadata">
		<summary>Repository mix</summary>
   		<description lang="en">Instala los Addons Mix.</description>
		<disclaimer></disclaimer>
		<platform>all</platform>
	</extension>
</addon>
</addons>
